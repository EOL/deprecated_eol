- editable ||= false
- item_editable = editable && !@collection.is_resource_collection?
%ul#collection_items.object_list
  - @collection_items.each do |item|
    - item_type = en_type(item)
    %li{:class => item_type}
      = render :partial => 'collection_items/show', :locals => { :collection_item => item, :item_type => item_type,
        :view_style => 'annotated', :item_ditable => item_editable }
