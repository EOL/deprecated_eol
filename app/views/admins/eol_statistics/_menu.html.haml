#tabs_bar
  %ul.tabs
    - [:index, :content_partners, :curators, :data_objects,
      :lifedesks, :marine, :page_richness, :users_data_objects].each do |report|
      - path_prefix = (report == :index) ? nil : report
      %li{:class => resource_is_active(report)}
        = link_to I18n.t(:menu_link, :scope => [:admins, :eol_statistics, report]),
          send([path_prefix, 'admin_eol_statistics_path'].compact.join('_'))
