%ul#collection_items.object_list
  - @collection_items.each do |item|
    - item_type = en_type(item)
    %li{:class => item_type}
      - if editable && !@collection.is_resource_collection?
        = render :partial => 'collection_items/edit', :locals => { :collection_item => item, :item_type => item_type, :parent_form => parent_form}
      - else
        = render :partial => 'collection_items/show', :locals => { :collection_item => item, :item_type => item_type, :view_style => 'annotations' }
